<module>
  <inherits name='arez.component.Component'/>
  <inherits name='org.realityforge.braincheck.BrainCheck'/>
  <inherits name='zemeckis.Zemeckis'/>

  <inherits name='akasha.Akasha'/>

  <!-- This is required while Keycloak usage is present -->
  <inherits name='org.realityforge.gwt.keycloak.Keycloak'/>

  <source path='' includes='*.java'/>
  <source path='events'/>
  <source path='messages'/>
  <source path='spy'/>

  <define-property name='replicant.check_invariants' values='true,false'/>
  <set-property name='replicant.check_invariants' value='false'/>

  <define-property name='replicant.check_api_invariants' values='true,false'/>
  <set-property name='replicant.check_api_invariants' value='false'/>

  <define-property name='replicant.enable_names' values='true,false'/>
  <set-property name='replicant.enable_names' value='false'/>

  <define-property name='replicant.validateChangeSetOnRead' values='true,false'/>
  <set-property name='replicant.validateChangeSetOnRead' value='false'/>

  <define-property name='replicant.validateEntitiesOnLoad' values='true,false'/>
  <set-property name='replicant.validateEntitiesOnLoad' value='false'/>

  <define-property name='replicant.enable_spies' values='true,false'/>
  <set-property name='replicant.enable_spies' value='false'/>

  <define-property name='replicant.enable_zones' values='true,false'/>
  <set-property name='replicant.enable_zones' value='false'/>

  <define-property name='replicant.enable_change_broker' values='true,false'/>
  <set-property name='replicant.enable_change_broker' value='false'/>

  <define-property name='replicant.logger' values='console,proxy,none'/>
  <set-property name='replicant.logger' value='console'/>
</module>
